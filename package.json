{
  "name": "react-procedural-context-menu",
  "repository": {
    "type": "git",
    "url": "https://github.com/Heaust-ops/react-procedural-context-menu.git"
  },
  "description": "A fully customizable procedural context menu for react",
  "author": "Heaust-ops",
  "license": "MIT",
  "private": false,
  "version": "0.1.3",
